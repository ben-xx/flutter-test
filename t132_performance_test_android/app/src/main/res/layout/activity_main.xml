<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <Button
            android:text="engine init"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/engine_init" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="50dp" android:onClick="engineInit" app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintTop_toBottomOf="@+id/button"/>
    <Button
            android:text="vm init"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/vm_init" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/engine_init" android:onClick="vmInit"/>
    <Button
            android:text="engine run"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/engine_run" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="50dp"
            app:layout_constraintTop_toBottomOf="@+id/vm_init" android:onClick="vmRun"/>
    <Button
            android:text="launch activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/launch_activity" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="50dp" app:layout_constraintTop_toBottomOf="@+id/engine_run"
            android:onClick="showActivity"/>
    <Button
            android:text="Load Library"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="50dp" android:onClick="libraryLoad"/>
</androidx.constraintlayout.widget.ConstraintLayout>